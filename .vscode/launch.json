{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "TRN BabylonJS",
            "url": "http://localhost:9000/TRN.html",
            "sourceMaps": true,
            "sourceMapPathOverrides": {
                "webpack:///./src/*": "${workspaceRoot}/babylonJS/src/*",
                "webpack:///../src/*": "${workspaceRoot}/src/*"
              }
        },
        {
            "type": "chrome",
            "request": "launch",
            "name": "TRN ThreeJS",
            "url": "http://localhost:9001/TRN.html",
            "sourceMaps": true,
            "sourceMapPathOverrides": {
                "webpack:///./src/*": "${workspaceRoot}/threeJS/src/*",
                "webpack:///../src/*": "${workspaceRoot}/src/*"
              }
        }
    ]
}